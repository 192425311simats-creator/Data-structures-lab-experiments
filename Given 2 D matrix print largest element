#include <stdio.h>

int main() {
    int m, n, i, j;
    scanf("%d %d", &m, &n);
    int mat[m][n];
    for(i = 0; i < m; i++)
        for(j = 0; j < n; j++)
            scanf("%d", &mat[i][j]);
    int max = mat[0][0];
    for(i = 0; i < m; i++)
        for(j = 0; j < n; j++)
            if(mat[i][j] > max)
                max = mat[i][j];
    printf("%d\n", max);
    return 0;
}
